@model Room

<div class="mydiv">
    <div class="col-sm-4 col-lg-4 col-md-4">
        <div class="thumbnail">
            <img src="http://placehold.it/64x64">
            <div class="caption">
                <h3 class="pull-right">@Model.Price.ToString("c")</h3>
                <h3>
                   @Html.ActionLink(Model.Name, "Details", "Room", new { id = Model.RoomId })
                </h3>
                <p>Available</p>
            </div>
        </div>
    </div>
</div>